{
  "name": "b2b-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "tsx --watch src/server.ts",
    "build": "next build",
    "build:temp": "ESLINT_CONFIG_FILE=.eslintrc.temp.js next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern='integration|middleware.*test' --runInBand",
    "test:unit": "jest --testPathPattern='.*\\.test\\.(ts|js)$' --testPathIgnorePattern='integration' --testPathIgnorePattern='middleware.*test'",
    "test:real-data": "jest --testPathPattern='integration|middleware.*test|gdpr|data-privacy|permissions' --runInBand --verbose",
    "test:pr": "npm run test:unit && npm run test:integration",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "generate:types": "tsx scripts/generate-types.ts",
    "pre-commit": "npm run format && npm run lint:fix && npm run typecheck",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm run test:ci",
    "validate": "npm run ci && npm run build"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/node": "^8.45.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/archiver": "^6.0.3",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/pg": "^8.15.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@workos-inc/authkit-nextjs": "^2.4.6",
    "archiver": "^7.0.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "json2csv": "^6.0.0-alpha.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "8.x"
  },
  "packageManager": "pnpm@8.15.6"
}
